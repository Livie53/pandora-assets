{
	"name": "pandora-tooling-template",
	"version": "0.1.0",
	"private": true,
	"repository": "git@github.com:Project-Pandora-Game/tooling-template.git",
	"license": "MIT",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "tsc -p ./tsconfig.src.json",
		"predev": "rimraf dist",
		"dev": "echo \"Error: no dev command specified\" && exit 1",
		"lint": "eslint --max-warnings 0 .",
		"lint:fix": "eslint --fix .",
		"type-check": "tsc -p . --noEmit",
		"type-check:test": "tsc -p ./tsconfig.test.json --noEmit",
		"type-check:src": "tsc -p ./tsconfig.src.json --noEmit",
		"test": "jest",
		"test:coverage": "jest --coverage",
		"test:watch": "jest --watch",
		"postinstall": "node scripts/postinstall.js"
	},
	"devDependencies": {
		"@babel/core": "^7.16.10",
		"@babel/preset-env": "^7.16.11",
		"@babel/preset-typescript": "^7.16.7",
		"@travellocal/babel-plugin-declare-const-enum": "^0.0.4",
		"@types/jest": "^27.4.0",
		"@typescript-eslint/eslint-plugin": "^4.29.0",
		"@typescript-eslint/parser": "^4.29.0",
		"babel-jest": "^27.4.6",
		"eslint": "^7.32.0",
		"jest": "^27.4.7",
		"rimraf": "^3.0.2",
		"tslib": "^2.3.0",
		"typescript": "^4.3.5"
	}
}
